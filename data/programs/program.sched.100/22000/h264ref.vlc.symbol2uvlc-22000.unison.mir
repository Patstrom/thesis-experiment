name:            symbol2uvlc
body:             |
  bb.0 (freq 1):
  
    BUNDLE {
      %r8 = L2_loadri_io %r0, 12
      %r4 = L2_loadri_io %r0, 16
      %r15 = A2_tfrsi 0
    }
    %r8 = S2_lsr_i_r_acc %r8, %r8, 31
