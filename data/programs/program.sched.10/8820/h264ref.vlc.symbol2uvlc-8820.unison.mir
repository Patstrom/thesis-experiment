body:             |
  bb.0 (freq 1):
  
    BUNDLE {
      %r15 = L2_loadri_io %r0, 12
      %r1 = L2_loadri_io %r0, 16
      %r4 = A2_tfrsi 1
      %r2 = A2_tfrsi -1
