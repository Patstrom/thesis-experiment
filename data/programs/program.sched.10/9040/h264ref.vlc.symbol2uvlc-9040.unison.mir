name:            symbol2uvlc
body:             |
  bb.0 (freq 1):
  
    BUNDLE {
      %r28 = L2_loadri_io %r0, 12
      %r4 = L2_loadri_io %r0, 16
      %r8 = A2_tfrsi -1
